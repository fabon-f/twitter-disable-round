{
    "manifest_version": 2,
    "name": "twitter-disable-round",
    "version": "1.1",
    "description": "あちこちが丸くなったのが気に食わん(川柳)",
    "content_scripts": [
        {
            "matches": [
                "https://*.twitter.com/*"
            ],
            "css": ["style.css"],
            "run_at": "document_start"
        }
    ],
    "applications": {
        "gecko": {
            "id": "twitter-disable-round@example.org",
            "strict_min_version": "46.0"
        }
    }
}
